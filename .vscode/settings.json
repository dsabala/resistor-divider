{
  "editor.formatOnSave": false,
  "C_Cpp.loggingLevel": "Warning",
  "cmake.sourceDirectory": "${workspaceFolder}",
  "cmake.buildDirectory": "${workspaceFolder}/build-${buildKit}-${buildType}",
  "cmake.copyCompileCommands": "${workspaceRoot}/compile_commands.json",
  "files.associations": {
    "cmath": "cpp",
    "cstdarg": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "type_traits": "cpp",
    "limits": "cpp",
    "*.tcc": "cpp",
    "cinttypes": "cpp",
    "typeinfo": "cpp",
    "cassert": "cpp"
  },
}